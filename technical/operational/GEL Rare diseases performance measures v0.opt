        <term_definitions code="at0004">
          <items id="description">Duration of head lift.</items>
          <items id="text">1.Head lift</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">The subject is unable to lift his or her head.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">The subject is able to lift his or her head for 1 to 9 seconds.</items>
          <items id="text">1-9 sec</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">The subject is able to lift his or her head for 10 to 29 seconds.</items>
          <items id="text">10-29 sec</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">The subject is able to lift his or her head for 30 to 59 seconds.</items>
          <items id="text">30-59 sec</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="description">The subject is able to lift his or her head for 60 to 119 seconds.</items>
          <items id="text">60-119 sec</items>
        </term_definitions>
        <term_definitions code="at0010">
          <items id="description">The subject is able to lift his or her head for more than 2 minutes.</items>
          <items id="text">more than 2 min</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">The total number of seconds the subject is able to lift his or her head.</items>
          <items id="text">Number of seconds head lift</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="description">Assessment of leg raise and object touch capabilities.</items>
          <items id="text">2.Leg raise/touch object</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">The subject is unable to lift leg off the table.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">The subject is able to clear the table but cannot touch object (examiner's hand).</items>
          <items id="text">Able but no touch</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">The subject is able to lift leg high enough to touch object (examiner's hand).</items>
          <items id="text">Able and touch</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Duration of straight leg lift.</items>
          <items id="text">3.Straight leg lift</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">The subject is unable to lift his or her leg.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">The subject is able to lift his or her leg for 1 to 9 seconds.</items>
          <items id="text">1-9 sec</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">The subject is able to lift his or her leg for 10 to 29 seconds.</items>
          <items id="text">10-29 sec</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">The subject is able to lift his or her leg for 30 to 59 seconds.</items>
          <items id="text">30-59 sec</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">The subject is able to lift his or her leg for 60 to 119 seconds.</items>
          <items id="text">60-119 sec</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">The subject is able to lift his or her leg for more than 2 minutes.</items>
          <items id="text">more than 2 min</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="description">The total number of seconds the subject is able to lift his or her leg.</items>
          <items id="text">Number of seconds leg lift</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Assesment of subject's ability to turn from supine to prone.</items>
          <items id="text">4.Supine to prone</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">The subject has difficulty even turning on one side, able to pull right arm under torso only slightly or not at all.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">The subject turns onto side faily easily, but cannot fully free right arm and is unable to fully assume a prone position.</items>
          <items id="text">Fairly easily, but unable to free arm</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">The subject turns onto side easily, has some difficulty freeing arm, but fully frees arm and fully assumes a prone position.</items>
          <items id="text">Easily with difficulty freeing arm</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">The subject turns over easily, fully frees right arm with no difficulty.</items>
          <items id="text">Easily</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="comment">Each selected item scores one point.</items>
          <items id="description">Assessment of the subject's ability to do sit-ups.</items>
          <items id="text">5.Sit-ups</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">The subject is able to sit up with hands on thighs with counterbalance.</items>
          <items id="text">Hands on thighs with counterbalance</items>
        </term_definitions>
        <term_definitions code="at0034">
          <items id="description">The subject is able to sit up with hands across chest and counterbalance.</items>
          <items id="text">Hands across chest with counterbalance</items>
        </term_definitions>
        <term_definitions code="at0035">
          <items id="description">The subject is able to sit up with hands behind the head and counterbalance.</items>
          <items id="text">Hands behind head with counterbalance</items>
        </term_definitions>
        <term_definitions code="at0036">
          <items id="description">The subject is able to sit up with hands across the thighs and without counterbalance.</items>
          <items id="text">Hands across thighs without counterbalance</items>
        </term_definitions>
        <term_definitions code="at0037">
          <items id="description">The subject is able to sit up with hands across the chest and without counterbalance.</items>
          <items id="text">Hands across chest without counterbalance</items>
        </term_definitions>
        <term_definitions code="at0038">
          <items id="description">The subject is able to sit up with hands behind the head and without counterbalance.</items>
          <items id="text">Hands behind head without counterbalance</items>
        </term_definitions>
        <term_definitions code="at0039">
          <items id="description">The total sit-up score calculated from the sum of each sit-up capability achieved, i.e. each achieved capability scores 1 point.</items>
          <items id="text">Total sit-up score</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="description">Assessment of the subject's ability to sit up from the supine position.</items>
          <items id="text">6.Supine to sit</items>
        </term_definitions>
        <term_definitions code="at0041">
          <items id="description">The subject is not able to sit up from supine position by himself or herself.</items>
          <items id="text">Unable by self</items>
        </term_definitions>
        <term_definitions code="at0042">
          <items id="description">The subject is able to sit up from supine position with much difficulty, very slow, struggles greatly, barely makes it.</items>
          <items id="text">Almost unable</items>
        </term_definitions>
        <term_definitions code="at0043">
          <items id="description">The subject is able to sit up from supine position with some difficulty, somewhat slow, struggles some.</items>
          <items id="text">Able</items>
        </term_definitions>
        <term_definitions code="at0044">
          <items id="description">The subject is able to sit up from supine position without any difficulty.</items>
          <items id="text">No difficulty</items>
        </term_definitions>
        <term_definitions code="at0045">
          <items id="description">Assessment of the subject's ability to raise and straighten wrists and arms.</items>
          <items id="text">7.Arm raise/straighten</items>
        </term_definitions>
        <term_definitions code="at0046">
          <items id="description">The subject cannot raise wrist up to the level of the A-C joint.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0047">
          <items id="description">The subject is able to raise wrists at least up to the level of the A-C joint, but not above top of head.</items>
          <items id="text">Up to A-C joint</items>
        </term_definitions>
        <term_definitions code="at0048">
          <items id="description">The subject is able to raise wrists above top of head, but cannot raise arm straight above head so that elbows are in full extension.</items>
          <items id="text">Above top of head</items>
        </term_definitions>
        <term_definitions code="at0049">
          <items id="description">The subject is able to raise arms straight above head so that elbows are in full extension.</items>
          <items id="text">Elbows in full extension</items>
        </term_definitions>
        <term_definitions code="at0050">
          <items id="description">Duration of ability to maintain wrists above top of head.</items>
          <items id="text">8.Arm raise duration</items>
        </term_definitions>
        <term_definitions code="at0051">
          <items id="description">The subject is unable to maintain wrists above top of head.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0052">
          <items id="description">The subject is able to maintain wrists above top of head for 1 to 9 seconds.</items>
          <items id="text">1-9 sec</items>
        </term_definitions>
        <term_definitions code="at0053">
          <items id="description">The subject is able to maintain wrists above top of head for 10 to 29 seconds.</items>
          <items id="text">10-29 sec</items>
        </term_definitions>
        <term_definitions code="at0054">
          <items id="description">The subject is able to maintain wrists above top of head for 30 to 59 seconds.</items>
          <items id="text">30-59 sec</items>
        </term_definitions>
        <term_definitions code="at0055">
          <items id="description">The subject is able to maintain wrists above top of head for more than 60 seconds.</items>
          <items id="text">more than 60 sec</items>
        </term_definitions>
        <term_definitions code="at0056">
          <items id="description">The total number of seconds the subject is able to maintain wrists above top of head.</items>
          <items id="text">Number of seconds arm rais</items>
        </term_definitions>
        <term_definitions code="at0057">
          <items id="description">Assessment of the subject's ability to go from a standing position to a sitting position on the floor.</items>
          <items id="text">9.Floor sit</items>
        </term_definitions>
        <term_definitions code="at0058">
          <items id="description">The subject is unable to go from a standing position to a sitting position on the floor. Afraid to even try, even if allowed a chair for support. Child fears that he or she will collapse, fall into a sit or harm self.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0059">
          <items id="description">The subject is able to go from a standing position to a sitting position on the floor, but needs to hold onto a chair for support during descent. Unable or unwilling to try if not allowed a chair for support.</items>
          <items id="text">Much difficulty</items>
        </term_definitions>
        <term_definitions code="at0060">
          <items id="description">The subject is able to go from a standing position to a sitting position on the floor without using a chair for support, but has at least some difficulty during descent. May need Gower's, descends somewhat slowly and apprehensively, may not have full contril or balance as maneuvers into a sit.</items>
          <items id="text">Some difficulty</items>
        </term_definitions>
        <term_definitions code="at0061">
          <items id="description">The subject is able to go from a standing position to a sitting position on the floor withour requiring any compensatory maneuvering.</items>
          <items id="text">No difficulty</items>
        </term_definitions>
        <term_definitions code="at0062">
          <items id="description">Assessment of the subject's ability to perform all fours maneuvers.</items>
          <items id="text">10.All fours maneuver</items>
        </term_definitions>
        <term_definitions code="at0063">
          <items id="description">The subject is unable to go from a prone to an all-fours position.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0064">
          <items id="description">The subject is barely able to assume and maintain an all-fours position and is unable to raise head to look straight ahead.</items>
          <items id="text">Barely able</items>
        </term_definitions>
        <term_definitions code="at0065">
          <items id="description">The subject is able to maintain all-fours position with back straight and head reaised (so as to look straight ahead), but cannot creep or crawl forward.</items>
          <items id="text">Maintain without ability to creep or crawl</items>
        </term_definitions>
        <term_definitions code="at0066">
          <items id="description">The subject is able to maintain all-fours position, look straight ahead and creep or crawl forward.</items>
          <items id="text">Maintain with creep or crawl</items>
        </term_definitions>
        <term_definitions code="at0067">
          <items id="description">The subject is able to maintain balance while lifting and extending one leg.</items>
          <items id="text">Maintains balance with lift and extend</items>
        </term_definitions>
        <term_definitions code="at0068">
          <items id="description">Assessment of the subject's ability to going from a kneeling position on the floor to a standing position.</items>
          <items id="text">11.Floor rise</items>
        </term_definitions>
        <term_definitions code="at0069">
          <items id="description">The subject is unable to go from a kneeling position on the floor to a standing position even if allowed to use a chair for support.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0070">
          <items id="description">The subject is able to go from a kneeling position on the floor to a standing position but needs to use a chair for support and is unable if not allowed to use a chair.</items>
          <items id="text">Much difficulty</items>
        </term_definitions>
        <term_definitions code="at0071">
          <items id="description">The subject is able to get up without using a chair for supports, but needs to place one or both hands on thighs or knees or floor (unable without using hands).</items>
          <items id="text">Moderate difficulty</items>
        </term_definitions>
        <term_definitions code="at0072">
          <items id="description">The subject is able to get up and does not need to place hands on knees, thighs or floor, but has at least some difficulty during ascent.</items>
          <items id="text">Mild difficulty</items>
        </term_definitions>
        <term_definitions code="at0073">
          <items id="description">The subject is able to go from a kneeling position on the floor to a standing position without any difficulty.</items>
          <items id="text">No difficulty</items>
        </term_definitions>
        <term_definitions code="at0074">
          <items id="description">Assessment of the subject's ability to rise up from a chair.</items>
          <items id="text">12.Chair rise</items>
        </term_definitions>
        <term_definitions code="at0075">
          <items id="description">The subject is unable to rise up from chair, even if allowed to place hands on chair seat.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0076">
          <items id="description">The subject is able to rise up from chair, but needs to place hands on sides of chair and is unable to rise up from chair if not allowed to place hands on sides of seat.</items>
          <items id="text">Much difficulty</items>
        </term_definitions>
        <term_definitions code="at0077">
          <items id="description">The subject is able to rise up from chair, but needs to place hands on knees or thighs, does not need to place hands on sides of seat.</items>
          <items id="text">Moderate difficulty</items>
        </term_definitions>
        <term_definitions code="at0078">
          <items id="description">The subject is able to rise up from chair and does not need to place hands on knees, thighs or seat, but has at least some difficulty during ascent.</items>
          <items id="text">Mild difficulty</items>
        </term_definitions>
        <term_definitions code="at0079">
          <items id="description">The subject is able to rise up from chair without any difficulty at all.</items>
          <items id="text">No difficulty</items>
        </term_definitions>
        <term_definitions code="at0080">
          <items id="description">Assessment of the subject's ability to step on a stool.</items>
          <items id="text">13.Stool step</items>
        </term_definitions>
        <term_definitions code="at0081">
          <items id="description">The subject is unable to step on a stool.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0082">
          <items id="description">The subject is able to step on a stool, but needs to place one hand on exam table or examiner's hand.</items>
          <items id="text">Much difficulty</items>
        </term_definitions>
        <term_definitions code="at0083">
          <items id="description">The subject is able to step on a stool and does not need to use exam table for support, but needs to hand on knee or thigh.</items>
          <items id="text">Some difficulty</items>
        </term_definitions>
        <term_definitions code="at0084">
          <items id="description">The subject is able to step on a stool and does not need to use exam table or hand on knee or thigh.</items>
          <items id="text">Able</items>
        </term_definitions>
        <term_definitions code="at0085">
          <items id="description">Assessment of the subject's ability to pick up a pencil off the floor.</items>
          <items id="text">14.Pick-up</items>
        </term_definitions>
        <term_definitions code="at0086">
          <items id="description">The subject is unable to bend over and pick up pencil off floor.</items>
          <items id="text">Unable</items>
        </term_definitions>
        <term_definitions code="at0087">
          <items id="description">The subject is able to pick up pencil off floor, but relies heavily on support gained by placing hands on knees or thighs.</items>
          <items id="text">Much difficulty</items>
        </term_definitions>
        <term_definitions code="at0088">
          <items id="description">The subject is able to pick up pencil off floor, but needs to at least minimally and briefly place hand(s) on knees or thighs for support, is somewhat slow.</items>
          <items id="text">Some difficulty</items>
        </term_definitions>
        <term_definitions code="at0089">
          <items id="description">The subject is able to pick up pencil off floor without any difficulty or compensatory maneuvers.</items>
          <items id="text">No difficulty</items>
        </term_definitions>
        <term_definitions code="at0090">
          <items id="description">The total score for the 14 maneuvers.</items>
          <items id="text">Total CMAS score</items>
        </term_definitions>
        <term_definitions code="at0091">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>

        <term_definitions code="at0000">
          <items id="description">Scale for the assessment and rating of ataxia.</items>
          <items id="text">SARA ataxia scale</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Event Series</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">A specific date and/or time which may be explicitly defined in a template or at run-time.</items>
          <items id="text">Point in time</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="comment">Proband is asked (1) to walk at a safe distance parallel to a wall including a half-turn (turn around to face the opposite direction of gait) and (2) to walk in tandem (heels to toes) without support.</items>
          <items id="description">Assessment of gait.</items>
          <items id="text">1. Gait</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">No difficulties in walking, turning and walking tandem (up to one misstep allowed).</items>
          <items id="text">Normal</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Slight difficulties which are only visible when walking 10 consecutive steps in tandem.</items>
          <items id="text">Slight difficulties</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">Clearly abnormal, tandem walking more than 10 steps not possible.</items>
          <items id="text">Clearly abnormal</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">Considerable staggering, difficulties in half-turn, but without support.</items>
          <items id="text">Considerable staggering</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="description">Marked staggering, intermittent support of wall required.</items>
          <items id="text">Marked staggering</items>
        </term_definitions>
        <term_definitions code="at0010">
          <items id="description">Severe staggering, permanent support of one stick or light support by one arm required.</items>
          <items id="text">Severe staggering</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Walking more than 10m with strong support only (two special sticks or stroller or accompanying person).</items>
          <items id="text">Walking more than 10m with strong support</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="description">Walking less than 10m with strong support only (two special sticks or stroller or accompanying person).</items>
          <items id="text">Walking less than 10m with strong support</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">Unable to walk, even supported.</items>
          <items id="text">Unable to walk</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="comment">Proband is asked to stand (1) in natural position, (2) with
feet together in parallel (big toes touching each other) and
(3) in tandem (both feet on one line, no space between
heel and toe). Proband does not wear shoes, eyes are
open. For each condition, three trials are allowed. Best
trial is rated.</items>
          <items id="description">Assessment of stance.</items>
          <items id="text">2. Stance</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Normal, able to stand in tandem for more than 10 seconds.</items>
          <items id="text">Normal</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Able to stand with feet together without sway, but
not in tandem for &gt; 10s.</items>
          <items id="text">Feet together without sway</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Able to stand with feet together for more than 10 seconds, but only 
with sway.</items>
          <items id="text">Feet together for more than 10 seconds</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">Able to stand for more than 10 seconds without support in natural
 position, but not with feet together.</items>
          <items id="text">Natural position more than 10 seconds without support</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Able to stand for more than 10 seconds in natural position only with
 intermittent support.</items>
          <items id="text">Natural position more than 10 seconds with intermittent support</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">Able to stand more than 10 seconds in natural position only with 
constant support of one arm.</items>
          <items id="text">Natural position more than 10 seconds with constant support</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Unable to stand for more than 10 seconds even with constant support
of one arm.</items>
          <items id="text">Unable to stand more than 10 seconds</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="comment">Proband is asked to sit on an examination bed without support of feet, eyes open and arms outstretched to the front.</items>
          <items id="description">Assessment of sitting.</items>
          <items id="text">3. Sitting</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="description">Normal, no difficulty sitting more than 10 seconds.</items>
          <items id="text">Normal</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Slight difficulties, intermittent sway.</items>
          <items id="text">Slight difficulties</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Constant sway, but able to sit more than 10 seconds without support.</items>
          <items id="text">Constant sway</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Able to sit for more than 10 seconds only with intermittent support.</items>
          <items id="text">Sit more than 10 seconds with intermittent support</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Unable to sit for more than 10 seconds without continuous support.</items>
          <items id="text">Unable to sit more than 10 seconds</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="comment">Speech is assessed during normal conversation.</items>
          <items id="description">Assessment of speech.</items>
          <items id="text">4. Speech disturbance</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="description">Assessment indicates that speech is normal.</items>
          <items id="text">Normal</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="description">Assessment indicates that there may be a speech disturbance.</items>
          <items id="text">Suggestion of speech disturbance</items>
        </term_definitions>
        <term_definitions code="at0031">
          <items id="description">Assessment indicates that speech is impaired, but easy to understand.</items>
          <items id="text">Impaired speech, but easy to understand</items>
        </term_definitions>
        <term_definitions code="at0032">
          <items id="description">Assessment indicates that occasional words are difficult to understand.</items>
          <items id="text">Occasional words difficult to understand</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">Assessment indicates that many words are difficult to understand.</items>
          <items id="text">Many words difficult to understand</items>
        </term_definitions>
        <term_definitions code="at0034">
          <items id="description">Assessment indicates that only single words are understandable.</items>
          <items id="text">Only single words understandable</items>
        </term_definitions>
        <term_definitions code="at0035">
          <items id="description">Assessment indicates that speech is unintelligible or the proband suffers from anarthria.</items>
          <items id="text">Speech unintelligible / anarthria</items>
        </term_definitions>
        <term_definitions code="at0036">
          <items id="comment">Proband sits comfortably. If necessary, support of feet and trunk is allowed. Examiner sits in front of proband and performs 5 consecutive sudden and fast pointing movements in unpredictable directions in a frontal plane, at about 50 % of proband´s reach. Movements have an amplitude of 30 cm and a frequency of 1 movement every 2 s. Proband is asked to follow the movements with his index finger, as fast and precisely as possible. Average performance of last 3 movements is rated.</items>
          <items id="description">Assessment of ability to follow movements with left index finger.</items>
          <items id="text">5L. Finger chase left</items>
        </term_definitions>
        <term_definitions code="at0037">
          <items id="description">Finger chase assessment indicates that no dysmetria is present.</items>
          <items id="text">No dysmetria</items>
        </term_definitions>
        <term_definitions code="at0038">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &lt;5cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &lt;5cm</items>
        </term_definitions>
        <term_definitions code="at0039">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &lt;15cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &lt;15cm</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &gt;15cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &gt;15cm</items>
        </term_definitions>
        <term_definitions code="at0041">
          <items id="description">Finger chase assessment indicates that proband is unable to perform 5 pointing movements.</items>
          <items id="text">Unable to perform 5 pointing movements</items>
        </term_definitions>
        <term_definitions code="at0047">
          <items id="comment">Proband sits comfortably. If necessary, support of feet and trunk is allowed. Examiner sits in front of proband and performs 5 consecutive sudden and fast pointing movements in unpredictable directions in a frontal plane, at about 50 % of proband´s reach. Movements have an amplitude of 30 cm and a frequency of 1 movement every 2 s. Proband is asked to follow the movements with his index finger, as fast and precisely as possible. Average performance of last 3 movements is rated.</items>
          <items id="description">Assessment of ability to follow movements with right index finger.</items>
          <items id="text">5R. Finger chase right</items>
        </term_definitions>
        <term_definitions code="at0048">
          <items id="description">Finger chase assessment indicates that no dysmetria is present.</items>
          <items id="text">No dysmetria</items>
        </term_definitions>
        <term_definitions code="at0049">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &lt;5cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &lt;5cm</items>
        </term_definitions>
        <term_definitions code="at0050">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &lt;15cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &lt;15cm</items>
        </term_definitions>
        <term_definitions code="at0051">
          <items id="description">Finger chase assessment indicates that dysmetria with under/overshooting target by &gt;15cm is present.</items>
          <items id="text">Dysmetria, under/overshooting target &gt;15cm</items>
        </term_definitions>
        <term_definitions code="at0052">
          <items id="description">Finger chase assessment indicates that proband is unable to perform 5 pointing movements.</items>
          <items id="text">Unable to perform 5 pointing movements</items>
        </term_definitions>
        <term_definitions code="at0054">
          <items id="description">Mean score for both sides of finger chase assessment.</items>
          <items id="text">5. Finger chase mean score</items>
        </term_definitions>
        <term_definitions code="at0055">
          <items id="comment">Proband sits comfortably. If necessary, support of feet and trunk is allowed. Proband is asked to point repeatedly with his index finger from his nose to examiner’s finger which is in front of the proband at about 90 % of proband’s reach. Movements are performed at moderate speed. Average performance of movements is rated according to the amplitude of the kinetic tremor.</items>
          <items id="description">Assessment of ability to point with left index finger from proband's nose to examiner's finger.</items>
          <items id="text">6L. Nose-finger test left</items>
        </term_definitions>
        <term_definitions code="at0056">
          <items id="description">Nose-finger test indicates that no tremor is present.</items>
          <items id="text">No tremor</items>
        </term_definitions>
        <term_definitions code="at0057">
          <items id="description">Nose-finger test indicates tremor with an amplitude less than 2 cm.</items>
          <items id="text">Tremor less than 2cm</items>
        </term_definitions>
        <term_definitions code="at0058">
          <items id="description">Nose-finger test indicates tremor with an amplitude less than 5 cm.</items>
          <items id="text">Tremor less than 5cm</items>
        </term_definitions>
        <term_definitions code="at0059">
          <items id="description">Nose-finger test indicates tremor with an amplitude more than 5 cm.</items>
          <items id="text">Tremor more than 5cm</items>
        </term_definitions>
        <term_definitions code="at0060">
          <items id="description">Nose-finger test indicates that proband is unable to perform 5 pointing movements.</items>
          <items id="text">Unable to perform 5 pointing movements</items>
        </term_definitions>
        <term_definitions code="at0062">
          <items id="comment">Proband sits comfortably. If necessary, support of feet and trunk is allowed. Proband is asked to point repeatedly with his index finger from his nose to examiner’s finger which is in front of the proband at about 90 % of proband’s reach. Movements are performed at moderate speed. Average performance of movements is rated according to the amplitude of the kinetic tremor.</items>
          <items id="description">Assessment of ability to point with right index finger from proband's nose to examiner's finger.</items>
          <items id="text">6R. Nose-finger test right</items>
        </term_definitions>
        <term_definitions code="at0063">
          <items id="description">Nose-finger test indicates that no tremor is present.</items>
          <items id="text">No tremor</items>
        </term_definitions>
        <term_definitions code="at0064">
          <items id="description">Nose-finger test indicates tremor with an amplitude less than 2 cm.</items>
          <items id="text">Tremor less than 2cm</items>
        </term_definitions>
        <term_definitions code="at0065">
          <items id="description">Nose-finger test indicates tremor with an amplitude less than 5 cm.</items>
          <items id="text">Tremor less than 5cm</items>
        </term_definitions>
        <term_definitions code="at0066">

        <term_definitions code="at0000">
          <items id="description">Measurment of the length, circumference, area or height of an identified body part, using a standardised procedure.</items>
          <items id="text">Measurement of Body Segment</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">Event Series</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Default, unspecified point in time which may be explicitly defined in a template or at run-time.</items>
          <items id="text">Any point in time event</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="comment">The term 'length' as used in this data element is also inclusive of breadth and depth. The name of the measurement is specified in the run-time name constraints.</items>
          <items id="description">Distance between two specified anatomical locations of an identified body part.</items>
          <items id="text">Length</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">Distance between the acromion process of the left scapula to the tip of the left olecranon, over the posterior surface of the upper arm.</items>
          <items id="text">Left Upper Arm Length</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">Distance between the left inguinal crease to the proximal border of the left patella, over the anterior surface of the left thigh, performed with the individual sitting.</items>
          <items id="text">Left Upper Leg Length</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">Distance between the right inguinal crease to the proximal border of the right patella, over the anterior surface of the right thigh, performed with the individual sitting.</items>
          <items id="text">Right Upper Leg Length</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">Distance between the acromion process of the right scapula to the tip of the right olecranon, over the posterior surface of the upper arm.</items>
          <items id="text">Right Upper Arm Length</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="description">Distance between the right anterior superior iliac spine (ASIS) and the right medial malleolus.</items>
          <items id="text">Right Leg Length</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Average measurement of all point in time 'Any Event' recordings. In anthropometric assessments, it is common for multiple readings to be taken and the average recorded.</items>
          <items id="text">Average measurement</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="comment">The name of the area is specified in the run-time name constraints.</items>
          <items id="description">Calculated area of an identified region or body segment.</items>
          <items id="text">Area</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="comment">For example: floor or chair seat.</items>
          <items id="description">Height of an identified body part from a fixed object.</items>
          <items id="text">Height</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Vertical distance between the sitting surface and the vertex of the head. A measure of trunk length, performed with the individual sitting erect.</items>
          <items id="text">Sitting Height</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Distance between vertex of head to the lowest part of the trunk, usually the perineum or lowest surface of the buttock. A measure of trunk length, usually conducted on infants under 2 years of age, performed with the individual lying supine.</items>
          <items id="text">Crown-Rump Length</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Distance around the girth of an identified body part at a specified anatomical location.</items>
          <items id="text">Circumference</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Measurement of the girth of the left upper arm at the mid point between the acromion process of the scapula and the olecranon process of the ulna. Also referred to as Mid-Upper Arm Circumference (MUAC).</items>
          <items id="text">Left Upper Arm Circumference (UAC)</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="description">Estimation of the area of the left upper arm, derived from the measurement of the upper arm circumference.</items>
          <items id="text">Left Mid-Upper Arm Muscle Area (MUAMA)</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Estimation of the area of the right upper arm, derived from the measurement of the upper arm circumference.</items>
          <items id="text">Right Mid-Upper Arm Muscle Area (MUAMA)</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="description">Distance between the left anterior superior iliac spine (ASIS) and the left medial malleolus.</items>
          <items id="text">Left Leg Length</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Measurement of the girth of the right upper arm at the mid point between the acromion process of the scapula and the olecranon process of the ulna. Also referred to as Mid-Upper Arm Circumference (MUAC).</items>
          <items id="text">Right Upper Arm Circumference (UAC)</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">Record the difference between two measurements of body segments. For example: a leg length discrepancy between left and right sides. May be a positive or negative numbe</items>
          <items id="text">Difference</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Details of the device used for measuring the body part.</items>
          <items id="text">Measuring Device</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="comment">For example, recording the absence of a limb due to amputation, or difficulties in obtaining a measurement.</items>
          <items id="description">Additional narrative about the measurements, not captured in the measurement data elements.</items>
          <items id="text">Comment</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="comment">This data element can be bound to a terminology to provide the valid names for use in a template or at run-time, or alternatively, names can be constrained in a template.</items>
          <items id="description">Distance between two specified anatomical locations (length), and anotomical location and a fixed object (height), or the girth of a body part (circumference).</items>
          <items id="text">Unspecified Measure</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="comment">This data element can be bound to a terminology to provide the valid names for use in a template or at run-time, or alternatively, names can be constrained in a template.</items>
          <items id="description">Calculated area of an region of the body.</items>
          <items id="text">Unspecified Area</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Measurement of the girth of the chest, usually at the level of the xiphoid notch.</items>
          <items id="text">Chest circumference</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="description">The difference between chest circumference between full exhalation and full inhalation. Always recorded as a positive measurement.</items>
          <items id="text">Chest expansion</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="description">Measurement taken during full exhalation.</items>
          <items id="text">Full exhalation</items>
        </term_definitions>
        <term_definitions code="at0031">
          <items id="description">Measurement of the girth of the hips, usually taken around the widest portion of the buttocks.</items>
          <items id="text">Hip circumference</items>
        </term_definitions>
        <term_definitions code="at0032">
          <items id="description">Measurement of the girth of the waist, usually at the approximate midpoint between the lower margin of the last palpable rib and the top of the iliac crest.</items>
          <items id="text">Waist circumference</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">Measurement taken during full inhalation.</items>
          <items id="text">Full inhalation</items>
        </term_definitions>
      </children>

							ELEMENT[at0284] occurrences matches {0..1} matches {	-- Gleason applicable
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0282] occurrences matches {0..*} matches {	-- Gleason

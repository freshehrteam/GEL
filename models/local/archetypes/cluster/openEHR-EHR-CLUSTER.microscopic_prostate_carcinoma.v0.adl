											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0296, 	-- Right anterior
													at0297, 	-- Right posterior
													at0298, 	-- Left anterior
													at0299]	-- Left posterior
												}
											}
										}
									}
									ELEMENT[at0288] occurrences matches {0..1} matches {	-- Quadrant
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0300, 	-- Apex
													at0301, 	-- Mid

definition
	CLUSTER[at0000] matches {	-- Fuhrman grade
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- Fuhrman grade
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0002, 	-- Grade 1
							at0003, 	-- Grade 2
							at0004, 	-- Grade 3
							at0005]	-- Grade 4

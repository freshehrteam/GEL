         <xs:element name="radiotherapy"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="radiotherapy-14972">
            <xs:annotation>
               <xs:documentation>For each course of radiotherapy, in addition to the core intervention data items, the following are required:</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="cancer-specific-treatments"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="cancer-specific-treatments-39084">

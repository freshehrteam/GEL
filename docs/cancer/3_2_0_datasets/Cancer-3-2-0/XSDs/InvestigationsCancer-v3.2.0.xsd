<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
    xmlns:gelCAN="https://genomicsengland.co.uk/xsd/cancer/3.2.0"
    xmlns="https://genomicsengland.co.uk/xsd/cancer/3.2.0"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="https://genomicsengland.co.uk/xsd/cancer/3.2.0" vc:minVersion="1.1"
    elementFormDefault="qualified">
    <xs:element name="investigations">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="metadata" type="metadata" minOccurs="1" maxOccurs="1"/>
                <xs:element name="subject" type="subject-29939" minOccurs="1" maxOccurs="unbounded"
                />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="metadata">
        <xs:sequence>
            <xs:element name="schema-name" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="InvestigationsCancer"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="schema-version" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="3.2.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="date-time" type="xs:dateTime" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        <p>The dateTime that this file was generated</p>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="source-organisation" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        <p>ODS code of the source organisation within the GMC sending the
                            message</p>
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="source-system" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        <p>Source system. Optional but may be used to supply source with information
                            if there are any issues with the submitted XML.</p>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="local-report-id" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>
                        <p>Source system report Id. Optional but may be used to supply source with
                            information if there are any issues with the submitted XML.</p>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="subject-29939">
        <xs:sequence>
            <xs:element name="participant-identifiers" type="participant-identifiers-39047"
                minOccurs="1" maxOccurs="1"/>
            <xs:element name="imaging" type="imaging-29093" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="sample-pathology" type="sample-pathology-14901" minOccurs="0"
                maxOccurs="unbounded"/>
            <xs:element name="genetic-results" type="genetic-results-29948" minOccurs="0"
                maxOccurs="unbounded"/>
            <xs:element name="next-generation-sequencing" type="next-generation-sequencing-14857"
                minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="circulating-tumour-markers" type="circulating-tumour-markers-40381"
                minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="investigation-report-other" type="investigation-report-other-33105"
                minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="participant-identifiers-39047">
        <xs:sequence>
            <xs:element name="person-identifier" minOccurs="1" maxOccurs="1"
                type="person-identifier-42125">
                <xs:annotation>
                    <xs:documentation>Choice of one of either NHS Number (Wales &amp; England) OR
                        CHI Number (Scotland) OR Health and Care Number (Northern
                        Ireland).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="participant-id" minOccurs="1" maxOccurs="1" type="participantid-3375">
                <xs:annotation>
                    <xs:documentation>Participant Identifier (supplied by Genomics
                        England)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="date-of-birth" minOccurs="1" maxOccurs="1" type="xs:date">
                <xs:annotation>
                    <xs:documentation>The date on which a PERSON was born or is officially deemed to
                        have been born.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="surname" minOccurs="1" maxOccurs="1" type="personfamilyname-321">
                <xs:annotation>
                    <xs:documentation>The participant's surname</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="forenames" minOccurs="1" maxOccurs="1" type="persongivenname-323">
                <xs:annotation>
                    <xs:documentation>The participant's forenames</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="person-identifier-42125">
        <xs:choice>
            <xs:element name="nhs-number" minOccurs="1" maxOccurs="1" type="nhsnumber-3378">
                <xs:annotation>
                    <xs:documentation>Validated NHS number for participant</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="chi-number" minOccurs="1" maxOccurs="1" type="chinumber-3380">
                <xs:annotation>
                    <xs:documentation>The COMMUNITY HEALTH INDEX NUMBER (CHI NUMBER) uniquely
                        identifies a PATIENT on the Community Health Index (Scotland) within the NHS
                        in Scotland. It is the equivalent of the NHS NUMBER in England and
                        Wales.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="health-and-care-number" minOccurs="1" maxOccurs="1"
                type="healthandcarenumber-42127">
                <xs:annotation>
                    <xs:documentation>Validated HEALTH AND CARE NUMBER (H&amp;C NUMBER). Uniquely
                        identifies a PATIENT within the NHS in Northern Ireland. It is the
                        equivalent of the NHS NUMBER in England and Wales.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="participantid-3375">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="\d{9}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="personfamilyname-321">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z0-9\s-']{1,50}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="persongivenname-323">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z0-9\s-']{1,50}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="nhsnumber-3378">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
            <xs:pattern value="[0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="chinumber-3380">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="healthandcarenumber-42127">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="investigations-14896">
        <xs:sequence>
            <xs:element name="participant-identifiers" minOccurs="1" maxOccurs="1"
                type="participant-identifiers-39047">
                <xs:annotation>
                    <xs:documentation>The following information is used to identify the participant
                        and must be included with all data submissions. For non-NHSE participants,
                        including overseas participants, who do not have an NHS number, the dummy
                        NHS number 2222222222 should be used.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging" minOccurs="0" maxOccurs="unbounded" type="imaging-29093">
                <xs:annotation>
                    <xs:documentation>If the investigation involves imaging, the following should be
                        included:</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sample-pathology" minOccurs="0" maxOccurs="unbounded"
                type="sample-pathology-14901">
                <xs:annotation>
                    <xs:documentation>Clinical event corresponding to a pathology report. 
                        
                        We expect to receive at least one or more Sample Pathology records for each participant and for each Diagnosis. 
                        
                        The Sample Pathology Event Date will be the date of final authorisation of the corresponding pathology report.
                        
                        The pathology report, as well as Event Details, should include Morphology, Topography, and, where applicable, Cancer Specific Tumour Markers, Cancer Specific Pathology and Cancer Specific Grading information.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="genetic-results" minOccurs="0" maxOccurs="unbounded"
                type="genetic-results-29948">
                <xs:annotation>
                    <xs:documentation>Enter all abnormal genetic results and all pertinent negative
                        results from this sample. Use one entry per gene.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="next-generation-sequencing" minOccurs="0" maxOccurs="unbounded"
                type="next-generation-sequencing-14857">
                <xs:annotation>
                    <xs:documentation>Next Generation Sequencing performed outside of Genomics
                        England.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="circulating-tumour-markers" minOccurs="0" maxOccurs="unbounded"
                type="circulating-tumour-markers-40381">
                <xs:annotation>
                    <xs:documentation>If applicable provide one of:</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="investigation-report-other" minOccurs="0" maxOccurs="unbounded"
                type="investigation-report-other-33105">
                <xs:annotation>
                    <xs:documentation>This class is a generic mechanism to include additional
                        investigation reports that are clinically relevant but have not been
                        specified within the current set of investigations. For every report please
                        include the name of the type of report and the ID of the report type i.e.
                        Name: Electrolytes panel - Blood ID: 55231-5 Reporting Standard: LOINC In
                        addition to the report name please provide one or more attributes that
                        correspond to the report attribute model below. These can either be test
                        results or metadata associated with the report.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="imaging-29093">
        <xs:sequence>
            <xs:element name="event-details" minOccurs="1" maxOccurs="1" type="event-details-40374">
                <xs:annotation>
                    <xs:documentation>The local ‘Event Reference’ combined with the Event Date, is
                        used to identify the actual participant event involved. GMCs must ensure
                        that local event references are not re-used to refer to different events,
                        therefore two different events on the same date can be easily identified.
                        GMCs must also ensure no two participants are given the same event
                        reference. If a second submission is received with the same local event
                        reference, then Genomics England will assume that this is an update
                        pertaining to the same participant event, and will be used in place of the
                        original submission; any data values previously supplied that are still
                        applicable must therefore be included.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="related-cancer-diagnoses" minOccurs="1" maxOccurs="1"
                type="related-cancer-diagnoses-40377">
                <xs:annotation>
                    <xs:documentation>Cancer diagnoses that led to the reported clinical event. More
                        than one diagnosis can be provided for the same event, e.g. where an event
                        pertains to more than one diagnosis.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sample-details" minOccurs="0" maxOccurs="1"
                type="sample-details-40380">
                <xs:annotation>
                    <xs:documentation>All sample investigations relating to germline or tumour
                        molecular genetics should have the following:</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-code" minOccurs="1" maxOccurs="1" type="imaging-code-40378">
                <xs:annotation>
                    <xs:documentation>Choice of SNOMED CT or NICIP imaging codes:</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cancer-specific-imaging" minOccurs="0" maxOccurs="1"
                type="cancer-specific-imaging-40379">
                <xs:annotation>
                    <xs:documentation>Submission of cancer specific risk imaging is optional
                        however, if cancer specific imaging is submitted, one of the following must
                        be provided:</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-modality" minOccurs="0" maxOccurs="1"
                type="cancerimagingmodality-393">
                <xs:annotation>
                    <xs:documentation>*IMAGING CODE (NICIP) and/or *IMAGING CODE (SNOMED CT) and/or
                        *CANCER IMAGING MODALITY and IMAGING ANATOMICAL SITE and ANATOMICAL SIDE
                        (IMAGING) is required. The type of imaging procedure used during an Imaging
                        or Radiodiagnostic Event for a Cancer Care Spell. NB: PET Scan also includes
                        PET-CT Scan.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="anatomical-site" minOccurs="0" maxOccurs="1"
                type="imaginganatomicalsite-395">
                <xs:annotation>
                    <xs:documentation>A classification of the part of the body that is the subject of an Imaging Or Radiodiagnostic Event.
                        The coding frame used is the OPCS-4 'Z' coding, plus two additional local codes:
                        Whole body CZ001
                        Multiple sites CZ002
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="anatomical-side" minOccurs="0" maxOccurs="1"
                type="anatomicalsideimaging-397">
                <xs:annotation>
                    <xs:documentation>The side of the body that is the subject of an Imaging or
                        Radiodiagnostic Event.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-report-reference" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>This is an internal reference that will allow your centre to
                        retrieve the imaging report associated with this imaging
                        event</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="image-file-reference" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>If not possible to submit Image File, please supply Image File
                        local reference, according to local imaging guidance.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ultrasound-examination-result" minOccurs="0" maxOccurs="1"
                type="ultrasoundexaminationresult-151">
                <xs:annotation>
                    <xs:documentation>Relates to COSD v7, CR6000. Result of the ultrasound
                        examination. For example in Breast Cancer, this will normally be the result
                        of the ultrasound examination of the breast undertaken at the first
                        outpatient appointment at the breast clinic. If the patient attends more
                        than one breast clinic, the result of each ultrasound examination of the
                        breast should be recorded.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-report-text" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>This is the full text provided in the imaging
                        report.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="tumour-id" minOccurs="1" maxOccurs="unbounded" type="tumourid-42261">
                <xs:annotation>
                    <xs:documentation>A locally allocated identifier for the participant's tumour. This should be unique for each tumour submitted from a patient.  Two tumours resected at the same time would have unique Tumour IDs. 
                        All sample reports and event reports that relate to a Genomics England tumour sample must have a locally allocated Tumour ID. Tumour IDs must be unique within the context of a GMC Clinic and should conform to the following convention:
                        Clinic ID proceeded by "_" proceeded by the local tumour identifier used to refer to a tumour, which must be between 1 and 16 alphanumeric characters
                        i.e. RN3_A098BC
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="event-details-40374">
        <xs:sequence>
            <xs:element name="event-date" minOccurs="1" maxOccurs="1" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Date of the clinical event or observation being reported e.g.
                        date biopsy was taken</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="event-reference" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Unique identifier for local record of clinical event or
                        observation</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="related-cancer-diagnoses-40377">
        <xs:sequence>
            <xs:element name="related-cancer-diagnosis-icd" minOccurs="1" maxOccurs="unbounded"
                type="primarydiagnosisicd-311">
                <xs:annotation>
                    <xs:documentation>Cancer diagnoses that led to the reported clinical event. More
                        than one diagnosis can be provided for the same event, e.g. where an event
                        pertains to more than one diagnosis.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="related-cancer-diagnosis-snomedct" minOccurs="0" maxOccurs="unbounded"
                type="diagnosiscodesnomedct-10594">
                <xs:annotation>
                    <xs:documentation>Optionally, provide the related cancer diagnosis as SNOMED CT
                        code as well as the ICD code. Related Cancer Diagnosis is the diagnosis that
                        led to the reported clinical event. More than one diagnosis can be provided
                        for the same event, e.g. where an event pertains to more than one
                        diagnosis.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="sample-details-40380">
        <xs:sequence>
            <xs:element name="local-sample-id" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The local identifier for the source sample</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="sample-taken-date" minOccurs="0" maxOccurs="1" type="xs:date">
                <xs:annotation>
                    <xs:documentation>The date upon which the sample was taken</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sample-receipt-date" minOccurs="0" maxOccurs="1" type="xs:date">
                <xs:annotation>
                    <xs:documentation>The date upon which the sample was received at the
                        laboratory</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="imaging-code-40378">
        <xs:choice>
            <xs:element name="imaging-code-snomedct" minOccurs="1" maxOccurs="1"
                type="snomedct-3610">
                <xs:annotation>
                    <xs:documentation>*IMAGING CODE (NICIP) and/or *IMAGING CODE (SNOMED CT) and/or
                        *CANCER IMAGING MODALITY and IMAGING ANATOMICAL SITE and ANATOMICAL SIDE
                        (IMAGING) is required. IMAGING CODE (NICIP) is the National Interim Clinical
                        Imaging Procedure Code Set code which is used to identify both the test
                        modality and body site of the test</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-code-nicip" minOccurs="1" maxOccurs="1"
                type="imagingcodenicip-389">
                <xs:annotation>
                    <xs:documentation>*IMAGING CODE (NICIP) and/or *IMAGING CODE (SNOMED CT) and/or
                        *CANCER IMAGING MODALITY and IMAGING ANATOMICAL SITE and ANATOMICAL SIDE
                        (IMAGING) is required. IMAGING CODE (NICIP) is the National Interim Clinical
                        Imaging Procedure Code Set code which is used to identify both the test
                        modality and body site of the test.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="cancer-specific-imaging-40379">
        <xs:choice>
            <xs:element name="imaging-breast" minOccurs="1" maxOccurs="1"
                type="imaging-breast-38959">
                <xs:annotation>
                    <xs:documentation>Priority COSD data items from BREAST - IMAGING. To carry
                        imaging mammogram, ultrasound and axilla ultrasound details for breast
                        cancer.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="imaging-cns" minOccurs="1" maxOccurs="1" type="imaging-cns-38986">
                <xs:annotation>
                    <xs:documentation>Priority COSD data items from CNS - IMAGING. To carry imaging
                        details for CNS cancer.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="imaging-breast-38959">
        <xs:sequence>
            <xs:element name="mammogram-result" minOccurs="1" maxOccurs="1"
                type="mammogramresult-137">
                <xs:annotation>
                    <xs:documentation>Result of the mammogram. This will normally be the result of
                        the mammogram taken at the first outpatient appointment at the breast
                        clinic. If the patient attends more than one breast clinic, the result of
                        each mammogram should be recorded.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="imaging-cns-38986">
        <xs:sequence>
            <xs:element name="lesion-location-radiological" minOccurs="0" maxOccurs="1"
                type="locationsurgical-282">
                <xs:annotation>
                    <xs:documentation>Radiologically determined anatomical location of lesion
                        (largest lesion if more than one) or where centred. This is recorded prior
                        to treatment.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number-of-lesions-radiological" minOccurs="0" maxOccurs="1"
                type="numberoflesionsradiological-268">
                <xs:annotation>
                    <xs:documentation>Radiologically determined number of lesions. (From UPPER GI -
                        STAGING - LIVER HCC)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="principal-diagnostic-imaging-type" minOccurs="0" maxOccurs="1"
                type="principaldiagnosticimagingtype-274">
                <xs:annotation>
                    <xs:documentation>Indicate the principal imaging procedure undertaken to
                        diagnose the tumour. NB: PET Scan also includes PET-CT
                        Scan</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="features-of-largest-lesion-radiological" minOccurs="0" maxOccurs="1"
                type="featuresoflargestlesionradiological-272">
                <xs:annotation>
                    <xs:documentation>Radiologically identified features of the largest lesion such
                        as density, necrosis recorded pre treatment. This may involve selection of
                        more than one value.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lesion-size-radiological" minOccurs="0" maxOccurs="1"
                type="lesionsizeradiological-270">
                <xs:annotation>
                    <xs:documentation>Radiological estimate in millimetres of the maximum diameter
                        of the tumour measured prior to treatment (largest lesion if more than one).
                        Record as "0" to indicate not assessable for diffuse tumours (e.g.
                        gliomatosis cerebri).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="sample-pathology-14901">
        <xs:sequence>
            <xs:element name="event-details" minOccurs="1" maxOccurs="1" type="event-details-40374">
                <xs:annotation>
                    <xs:documentation>The local ‘Event Reference’ combined with the Event Date, is
                        used to identify the actual participant event involved. GMCs must ensure
                        that local event references are not re-used to refer to different events,
                        therefore two different events on the same date can be easily identified.
                        GMCs must also ensure no two participants are given the same event
                        reference. If a second submission is received with the same local event
                        reference, then Genomics England will assume that this is an update
                        pertaining to the same participant event, and will be used in place of the
                        original submission; any data values previously supplied that are still
                        applicable must therefore be included.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="related-cancer-diagnoses" minOccurs="1" maxOccurs="1"
                type="related-cancer-diagnoses-40377">
                <xs:annotation>

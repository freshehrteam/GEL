               <xs:documentation>GMC number of consultant with responsibility for the patient's clinical care</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="full-name-not-consultant" minOccurs="0" maxOccurs="1">
            <xs:annotation>

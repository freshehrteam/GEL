            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="contact-number" minOccurs="0" maxOccurs="1"
            type="uktelephonenumber-40375">
            <xs:annotation>
               <xs:documentation>Phone number for the consultant.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="hospital-of-responsible-consultant" minOccurs="0" maxOccurs="1"
            type="organisationsitecode-3371">
            <xs:annotation>
               <xs:documentation>ODS code of the hospital to which the consultant is contracted under their MAIN SPECIALTY for the purposes of the current work.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
